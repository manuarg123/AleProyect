<div class="row g-3">
      <div class="col 3"></div>
      <div class="col 6">
        <center><h2>Agregar Paciente</h2></center>
        <form action="/api/usuario/paciente" method="POST">
          <div class="form-group">
            <label for="nombres">Nombre</label>
            <input
              type="text"
              class="form-control"
              id="nombres"
              name="nombre"
              placeholder="Ingresar nombre"
            />
          </div>
          <div class="form-group">
            <label for="apellidos">Apellido</label>
            <input
              type="text"
              class="form-control"
              id="apellidos"
              name="apellido"
              placeholder="Ingrar apellido"
            />
          </div>
          <div class="form-group">
            <label for="dnis">Dni</label>
            <input
              type="text"
              class="form-control"
              id="dnis"
              name="dni"
              placeholder="Ingresar DNI"
            />
          </div>
          <div class="form-group">
            <label for="fecha_nacimiento">Fecha Nacimiento</label>
            <input type="date" class="form-control" name="fecha_nacimiento" />
          </div>
          <div class="form-group">
            <label for="">Direccion</label>
            <input
              type="text"
              class="form-control"
              id=""
              name="direccion"
              placeholder="Ingresar Dirección"
            />
          </div>
          <div class="form-group">
            <label for="">Localidad</label>
            <input
              type="text"
              class="form-control"
              id=""
              name="localidad"
              placeholder="Ingresar localidad"
            />
          </div>
          <div class="form-group">
            <label for="emails">Email</label>
            <input
              type="text"
              class="form-control"
              id="emails"
              name="email"
              placeholder="Ingrese un email"
            />
          </div>
          <div class="form-group">
            <label for="">Telefono 1</label>
            <input
              type="text"
              class="form-control"
              id=""
              name="telefono_1"
              placeholder="Ingresar teléfono 1"
            />
          </div>
          <div class="form-group">
            <label for="">Telefono 2</label>
            <input
              type="text"
              class="form-control"
              id=""
              name="telefono_2"
              placeholder="Ingresar teléfono 2"
            />
          </div>
          <div class="form-group">
            <label for="">Obra Social</label>
            <input
              type="text"
              class="form-control"
              id=""
              name="obra_social"
              placeholder="Ingresar obra social"
            />
          </div>
          <div class="form-group">
            <label for="">Identificación obra social</label>
            <input
              type="text"
              class="form-control"
              id=""
              name="id_obra_social"
              placeholder="Ingresar identificacion obra social"
            />
          </div>
          <center><input type="submit" style="margin-top:10px!important;" class="btn btn-outline-success btn-sm" value="Registrar"/>
          
        </form>
           <form action="/api/usuario/?_method=GET" method="GET">
                <input type="hidden" value="GET" name="_method" />
                <button
                  class="btn btn-outline-primary btn-sm"
                  style="margin-top:10px!important"
                  type="submit"
                >Volver</button>
        </form>   
        </center>  
      </div>
      <div class="col 3"></div>
</div>